 local modules = peripheral.find("neuralInterface")
calcGravity()
while true do
 
local test = modules.getBlockMeta(0, -2, 0)
local test1 = modules.getBlockMeta(0, -3, 0)
local meta = modules.getMetaOwner()
if meta.isSneaking then
modules.launch(meta.yaw,meta.pitch, 4)
print(textutils.serialise(test))
sleep(0.2)
else  
sleep(0.001)
end
local function calcGravity()
    
    local meta = modules.getMetaOwner()
    modules.launch(0, -90, 4)
    os.sleep(1)
    meta = modules.getMetaOwner()
    local speedY = meta.motionY
    os.sleep(1)
    meta = modules.getMetaOwner()
    local acceleration = meta.motionY - speedY
    print("Gravity on this planet is "..acceleration.." blocks per second.")
    stop = true 
end
